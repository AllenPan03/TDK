{
	"editor.insertSpaces": false,
	"[yaml]": {
		"editor.insertSpaces": true,
		"editor.tabSize": 2
	},
	"html.format.extraLiners": "",
	"html.format.maxPreserveNewLines": null,
	"html.format.wrapLineLength": 0,
	"html.format.contentUnformatted": "pre,code,textarea,svg",
	"beautify.config": ".vscode/.jsbeautifyrc",
	"beautify.language": {
		"js": [],
		"css": [
			"css",
			"scss",
			"sass",
			"less",
			"styl"
		],
		"html": []
	},
	"eslint.validate": [
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact"
	],
	"eslint.options": {
		"configFile": ".vscode/.eslintrc.json"
	},
	"stylelint.configFile": ".vscode/.stylelintrc.json",
	"stylelint.validate": [
		"css",
		"html",
		"less",
		"markdown",
		"postcss",
		"sass",
		"scss",
		"source.css.styled",
		"source.markdown.math",
		"styled-css",
		"sugarss",
		"svelte",
		"vue",
		"vue-html",
		"vue-postcss",
		"xml",
		"xsl"
	]
}